pipeline{
    
    agent any
    
    stages{
        
        stage("Build"){
            steps{
                echo("Build")
            }
        }
        
        stage("Run UTs"){
            steps{
                echo("Run Uts")
            }
        }
        
        stage("Deploy to dev"){
            steps{
                echo("dev deployment")
            }
        }
        
        stage("Deploy to QA"){
            steps{
                echo("qa deployment")
            }
        }
        
        stage("Run Automation Reg Test"){
            steps{
                echo("Run Automation Reg Test")
            }
        }
        
        stage("Deploy to Stage"){
            steps{
                echo("stage deployment")
            }
        }
        
        stage("Deploy to PROD"){
            steps{
                echo("prod deployment")
            }
        }
        
    }
    
}